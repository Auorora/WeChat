.content{
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    background: url(../../assets/image/bg1.png) repeat;
    padding-top: 30rpx;
}
.item{
    width:90%;
    line-height: 100px;
    margin-top: 5rpx;
    padding-left: 3rpx;
    margin-bottom: 3rpx;
    border:1px solid #da7c0c;
    border-radius: 3px;
    background: #f78d1d;
}